<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.PossibleSolution.covid_19tracker">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/covid19"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.PossibleSolution.covid_19tracker.ContentActivity">
        </activity>
        <activity android:name="com.PossibleSolution.covid_19tracker.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.PossibleSolution.covid_19tracker.MainActivity"></activity>
    </application>

</manifest>